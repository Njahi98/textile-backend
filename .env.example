PORT=5000                        # Server listening port
NODE_ENV=development            # Environment (development, production, test)
FRONTEND_URL=http://localhost:3000  # Frontend URL for CORS


DATABASE_URL="postgresql://user:password@localhost:5432/textile_db?schema=public"  # PostgreSQL connection string


JWT_SECRET=your_jwt_secret_key                # Secret key for JWT token generation
JWT_REFRESH_SECRET=your_refresh_secret_key    # Secret key for refresh token generation


GOOGLE_CLIENT_ID=your_google_client_id          # Google OAuth 2.0 Client ID
GOOGLE_CLIENT_SECRET=your_google_client_secret  # Google OAuth 2.0 Client Secret


GOOGLE_APP_USER=your.email@gmail.com           # Gmail account for sending emails
GOOGLE_APP_PASSWORD=your_app_specific_password # App password from Google Account settings


CLOUDINARY_CLOUD_NAME=your_cloud_name  # Cloudinary cloud name from dashboard
CLOUDINARY_API_KEY=your_api_key        # Cloudinary API Key from dashboard
CLOUDINARY_API_SECRET=your_api_secret  # Cloudinary API Secret from dashboard


GEMINI_API_KEY=your_gemini_api_key    # Google Gemini API key for AI features

